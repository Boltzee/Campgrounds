<% if(success && success.length) {%>
	<div class="alert alert-success alert-dismissible fade show" role="alert" style="margin-top: 60px z-index: 1000;">
		<%= success %>
		<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
	</div>
<% }%>

<% if(error && error.length) {%>
	<div class="alert alert-danger alert-dismissible fade show mt-3" role="alert" style="margin-top: 60px z-index: 1000;">
		<%= error %>
		<button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
	</div>
<% }%>